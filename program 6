#include <iostream> 
using namespace std; 
void display(int a) { 
cout << "Displaying int: " << a << endl; 
} 
void display(double b) { 
cout << "Displaying double: " << b << endl; 
} 
void display(int a, string b) { 
cout << "Displaying int and string: " << a << ", " << b << endl; 
} 
void display() { 
cout << "Displaying nothing" << endl; 
} 
int main() { 
display();                    
display(10);                 
display(3.14);               
display(5, "Hello World");   
return 0; 
} 
