#include <iostream> 
using namespace std; 
class Student { 
public: 
    string name; 
    int age; 
    Student(string n, int a) { 
        name = n; 
        age = a; 
    } 
    Student(const Student &s) { 
                                                                                    Page No:30 
        cout << "Copy constructor is called!" << endl; 
        name = s.name; 
        age = s.age; 
    } 
     void display() { 
        cout << "Name: " << name << ", Age: " << age << endl; 
    } 
}; 
int main() { 
    Student s1("Sivani", 20);    
    Student s2 = s1;             
    s1.display(); 
    s2.display(); 
    return 0; 
} 
